  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "UpdateUserDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          }
        },
        "required": [
          "id"
        ]
      },
      "PermissionDto": {
        "type": "object",
        "properties": {
          "action": {
            "type": "string",
            "enum": [
              "read",
              "update",
              "create",
              "delete"
            ]
          },
          "resource": {
            "type": "string",
            "enum": [
              "auth",
              "products",
              "users",
              "messages",
              "chat"
            ]
          }
        },
        "required": [
          "action",
          "resource"
        ]
      },
      "CreateUserDto": {
        "type": "object",
        "properties": {
          "phoneNumber": {
            "type": "string"
          },
          "meliCode": {
            "type": "string"
          },
          "permissions": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PermissionDto"
            }
          }
        },
        "required": [
          "phoneNumber",
          "meliCode",
          "permissions"
        ]
      },
      "SignUpDto": {
        "type": "object",
        "properties": {
          "phoneNumber": {
            "type": "string"
          },
          "meliCode": {
            "type": "string"
          },
          "permissions": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PermissionDto"
            }
          }
        },
        "required": [
          "phoneNumber",
          "meliCode",
          "permissions"
        ]
      },
      "SignInDto": {
        "type": "object",
        "properties": {
          "phoneNumber": {
            "type": "string"
          }
        },
        "required": [
          "phoneNumber"
        ]
      },
      "VerifyOtpDto": {
        "type": "object",
        "properties": {
          "phoneNumber": {
            "type": "string"
          },
          "meliCode": {
            "type": "string"
          },
          "permissions": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PermissionDto"
            }
          },
          "otp": {
            "type": "string"
          }
        },
        "required": [
          "phoneNumber",
          "meliCode",
          "permissions",
          "otp"
        ]
      },
      "CreateProductDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Laptop"
          },
          "price": {
            "type": "number",
            "example": 1500
          },
          "companyId": {
            "type": "string",
            "example": "company123"
          },
          "categories": {
            "example": [
              "Electronics",
              "Computers"
            ],
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "description": {
            "type": "string",
            "example": "High-end gaming laptop with RTX 3080."
          },
          "userAgent": {
            "type": "string",
            "example": "Mozilla/5.0"
          },
          "ip": {
            "type": "string",
            "example": "192.168.1.1"
          }
        },
        "required": [
          "name",
          "price",
          "companyId",
          "categories",
          "description",
          "userAgent",
          "ip"
        ]
      },
